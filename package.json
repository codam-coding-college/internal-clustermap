{
  "name": "internal-clustermap",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "elm make src/elm/clustermap/Main.elm --optimize --output=public/js/interactive_maps.js && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^4.0.0",
    "cors": "^2.8.5",
    "next": "12.2.0",
    "react": "18.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "autoprefixer": "^10.4.7",
    "eslint": "8.18.0",
    "eslint-config-next": "12.1.6",
    "postcss": "^8.4.14",
    "prisma": "^4.0.0",
    "react-dom": "18.2.0",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "semantic-release": "^19.0.3",
    "@codedependant/semantic-release-docker": "^2.2.0",
    "tailwindcss": "^3.1.5",
    "typescript": "4.7.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codam-coding-college/internal-clustermap.git"
  },
  "release": {
    "branches": "main",
    "plugins": [
      ["@codedependant/semantic-release-docker", {
        "dockerTags": ["latest", "{{version}}", "{{major}}-latest'","{{major}}.{{minor}}"],
        "dockerImage": "internal-clustermap",
        "dockerFile": "Dockerfile",
        "dockerRegistry": "ghcr.io",
        "dockerProject": "codam-coding-college",
        "dockerLogin": false
      }]
    ]
  }
}
